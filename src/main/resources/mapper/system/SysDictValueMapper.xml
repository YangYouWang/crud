<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.github.yangyouwang.crud.system.mapper.SysDictValueMapper">
  <resultMap id="BaseResultMap" type="io.github.yangyouwang.crud.system.entity.SysDictValue">
    <id column="id" jdbcType="BIGINT" property="id" />
    <result column="dict_type_id" jdbcType="BIGINT" property="dictTypeId" />
    <result column="dict_value_key" jdbcType="VARCHAR" property="dictValueKey" />
    <result column="dict_value_name" jdbcType="VARCHAR" property="dictValueName" />
    <result column="order_num" jdbcType="INTEGER" property="orderNum" />
    <result column="create_by" jdbcType="VARCHAR" property="createBy" />
    <result column="create_time" jdbcType="TIMESTAMP" property="createTime" />
    <result column="update_by" jdbcType="VARCHAR" property="updateBy" />
    <result column="update_time" jdbcType="TIMESTAMP" property="updateTime" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
  </resultMap>
  <sql id="Base_Column_List">
    id, dict_type_id, dict_value_key, dict_value_name, order_num, create_by, create_time, 
    update_by, update_time, remark
  </sql>
  <insert id="insertBatch" parameterType="java.util.List">
    insert into sys_dict_value (
        id,dict_type_id,dict_value_key,dict_value_name,order_num,remark
    )
    values
    <foreach collection="list" item="item" separator=",">
      (
        #{item.id},#{item.dictTypeId},#{item.dictValueKey},#{item.dictValueName},#{item.orderNum},#{item.remark}
      )
    </foreach>
    ON DUPLICATE KEY UPDATE
    dict_type_id=values(dict_type_id),
    dict_value_key=values(dict_value_key),
    dict_value_name=values(dict_value_name),
    order_num=values(order_num),
    remark=values(remark)
  </insert>
</mapper>